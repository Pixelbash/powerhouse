{
    "name": "pixelbash/powerhouse",
    "description": "Powerhouse theme",
    "authors": [
        {
            "name": "Joe S",
            "email": "joe@pixelbash.co.nz"
        }
    ],
    "repositories": [
      {
        "type":"composer",
        "url":"http://wpackagist.org"
      }
    ],
    "require": {
      "abeautifulsite/simpleimage": "*",
      "jarednova/timber": "*",
      "wpackagist-plugin/advanced-custom-fields": "*",
      "wpackagist-plugin/multiple-post-thumbnails": "*",
      "wpackagist-plugin/options-framework": "*"
    },
    "extra": {
      "installer-paths": {
        "../../plugins/{$name}/": ["jarednova/timber", "type:wordpress-plugin"]
      }
    }
}
